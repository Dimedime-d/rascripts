        && just_collected_star(star_index=1, trigger=1),
    id = 217006,
    badge = "240079"
)

achievement(
    title = "Toxic Tempo",
    description = format("Collect the star '{0}' without going below half HP (indicator appears in pipe area).", stars[2]["star name"]),
